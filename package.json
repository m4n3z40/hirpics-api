{
  "name": "hirpics-api",
  "version": "0.0.1",
  "description": "The API server for the hirpics mobile app.",
  "repository": "https://m4n3z40@bitbucket.org/m4n3z40/hirpics-api.git",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate-up": "npm run migrate up",
    "migrate-down": "npm run migrate down",
    "migrate": "db-migrate --config ./config/database.json",
    "start": "node ./server.js",
    "stop": "if pkill -f \"node ./server.js\"; then echo \"Last process kiled.\"; fi;",
    "watch": "watch \"npm stop && npm start\" ."
  },
  "keywords": [
    "API",
    "hirpics",
    "pictures",
    "photos",
    "app"
  ],
  "author": "Allan Baptista (allan.marques@ymail.com)",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^2.10.2",
    "body-parser": "^1.14.1",
    "db-migrate": "^0.9.23",
    "express": "^4.13.3",
    "helmet": "^0.12.0",
    "mime-types": "^2.1.7",
    "mkdirp": "^0.5.1",
    "multer": "^1.0.6",
    "mysql": "^2.9.0",
    "node-geocoder": "^3.4.0"
  },
  "devDependencies": {
    "watch": "^0.16.0"
  }
}
